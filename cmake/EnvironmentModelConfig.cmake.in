@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Ensure dependencies are available
find_dependency(OpenMP)

if(@INSTALL_boost_src_FROM_SYSTEM@)
    find_dependency(Boost 1.70.0)
endif()

if(@INSTALL_Eigen3_FROM_SYSTEM@)
    find_dependency(Eigen3 3.3.7)
endif()

if(@INSTALL_spdlog_FROM_SYSTEM@)
    find_dependency(spdlog 1.8.0)
endif()

if(@INSTALL_range-v3_FROM_SYSTEM@)
    find_dependency(range-v3 0.6.0)
endif()

if(@INSTALL_yaml-cpp_FROM_SYSTEM@)
    find_dependency(yaml-cpp 0.6.0)
endif()

# Add targets
include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

check_required_components("@PROJECT_NAME@")
